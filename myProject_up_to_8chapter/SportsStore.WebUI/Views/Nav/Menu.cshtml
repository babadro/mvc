@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Product")

@foreach (var link in Model)
{
    @Html.RouteLink(
    link,
    new
    {
        controller = "Product",
        action = "List",
        page = 1,
        category = link
    },
    new
    {
        @class = ViewBag.SelectedCategory == link ? "selected" : null
    })
}